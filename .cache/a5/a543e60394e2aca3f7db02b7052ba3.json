color-dark: #046582;\n}\n\n\n*, *:before, *:after {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  background: var(--bg-color);\n  color: #fff;\n  font-family: 'Fira Sans Extra Condensed', sans-serif;\n  font-weight: 400;\n  font-style: normal;\n  text-align: center;\n  min-height: 150vh;\n}\n\nh1 {\n  font-family: 'Merriweather Sans', sans-serif;\n  font-weight: 700;\n  font-style: italic;\n  font-size: 5rem;\n  color: #001f3f;\n  margin: 2rem 1rem;\n}\n\nsection.pomodoro {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 600px;\n  margin: 0 auto;\n  padding: 1rem;\n  border-radius: 1rem;\n  background: var(--bg-color);\n  box-shadow:  8px 8px 16px #cf7b74,\n               -8px -8px 16px #ffa79e;\n}\n\nsection.pomodoro .status {\n  font-size: 3rem;\n  color: #fff;\n\n}\n\nsection.pomodoro .timer {\n  color: #001f3f;\n  font-size: 6rem;\n  font-weight: 600;\n  font-style: italic;\n  letter-spacing: .2rem;\n}\n\n.timer-btn, .reset-btn {\n  color: #fff;\n  font-size: 1.5rem;\n  font-family: 'Merriweather Sans', sans-serif;\n  text-transform: uppercase;\n  padding: 1rem;\n  min-width: 4rem;\n  border-radius: 2rem;\n  cursor: pointer;\n  transition: all .2s ease-out;\n  border: 1px solid transparent;\n}\n\n.timer-btn.start {\n  background-color: #046582;\n  border: 1px solid #6e7582;\n}\n\n.timer-btn.pause {\n  background-color: #af605a;\n  border: 1px solid var(--bg-color);\n}\n\n.timer-btn:hover, .reset-btn:hover {\n  transform: translateY(-.2rem);\n}\n\n.settings {\n  width: 600px;\n  display: grid;\n  grid-template-columns: 1fr 1fr 1fr;\n  place-items: center;\n  margin: 2rem auto;\n}\n\n.work-duration h3, .break-duration h3 {\n  font-size: 2rem;\n}\n\n.settings .duration-display {\n  font-size: 3rem;\n  color: #001f3f;\n}\n\n.settings .duration-btn {\n  color: #fff;\n  width: 4rem;\n  height: 4rem;\n  font-family: 'Merriweather Sans', sans-serif;\n  font-size: 2.5rem;\n  text-align: center;\n  border-radius: 50%;\n  border: 0;\n  padding: 0;\n  margin: 1rem;\n  background: var(--bg-color);\n  box-shadow:  5px 5px 10px #d37e77,\n               -5px -5px 10px #ffa49b;\n  cursor: pointer;\n  will-change: transform;\n  transition: .3s all ease-out;\n}\n\n.settings .duration-btn:hover {\n  background: #af605a;\n  transform: scale(1.05);\n}\n\n.settings .reset-btn {\n  background: #001f3f;\n  border: 1px solid #fff;\n}\n\n.to-do {\n  display: grid;\n  place-content: center;\n  grid-template-columns: 1fr 12fr 1fr;\n  grid-gap: 1rem;\n  width: 600px;\n  margin: 0 auto 4rem;\n  padding: 1rem;\n  border-radius: 1rem;\n  background: var(--bg-color);\n  box-shadow:  8px 8px 16px #cf7b74,\n               -8px -8px 16px #ffa79e;\n}\n\n.to-do h2 {\n  font-size: 4rem;\n  grid-column: 1 / -1;\n}\n\n.to-do form {\n  grid-column: 2;\n  display: grid;\n  grid-template-columns: 4fr 1fr;\n  grid-gap: .5rem;\n  place-content: center;\n  grid-template-rows: 2.5rem;\n}\n\n.to-do form input, .to-do form button {\n  padding: .5rem;\n  border-radius: .5rem;\n  border: 0;\n  /*border: 2px solid var(--bg-color-light);*/\n}\n\n.to-do form input {\n  background-color: rgba(255, 255, 255, .9);\n  font-size: 1rem;\n}\n\n.to-do form button {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-color: #046582;\n  color: #fff;\n  font-size: 1.5rem;\n  cursor: pointer;\n  will-change: transform;\n  transition: all .4s ease;\n}\n\n.to-do form button:hover {\n  transform: scale(1.01);\n}\n\n.to-do ul.task-list {\n  grid-column: 2;\n  list-style-type: none;\n  transition: all .5s ease;\n}\n\n.to-do ul.task-list li {\n  display: grid;\n  grid-template-columns: .1fr 8fr 1fr;\n  place-content: center;\n  font-size: 1.5rem;\n  text-align: left;\n  padding: 1rem;\n  margin: .5rem 0;\n  color: #001f3f;\n  background: rgba(255, 255, 255, .9);\n  border: 2px solid var(--bg-color-light);\n  border-radius: .5rem;\n}\n\n.to-do ul.task-list li input[type=\"checkbox\"] {\n  opacity: 0;\n}\n\n.to-do ul.task-list li label {\n  align-self: center;\n  cursor: pointer;\n  text-decoration: none;\n}\n\n.to-do ul.task-list li.completed label {\n  text-decoration: line-through;\n}\n\n.to-do ul.task-list li button {\n  /*background: none;*/\n  background-color: #001f3f;\n  background: linear-gradient(90deg, rgba(0,31,63,1) 0%, rgba(0,31,63,1) 10%, rgba(132,150,169,1) 10%, rgba(132,150,169,1) 30%, rgba(0,31,63,1) 30%);\n  background-size: 100%;\n  background-repeat: repeat;\n  -webkit-background-clip: text;\n  -webkit-text-fill-color: transparent; \n  font-size: 2rem;\n  border: 0;\n  padding: 0;\n  margin: 0;\n  cursor: pointer;\n  /*animation: simple-text-gradient-rev .6s ease forwards;*/\n}\n\n.to-do ul.task-list li button:hover {\n  animation: simple-text-gradient-rev .6s ease forwards;\n}\n\n@keyframes simple-text-gradient {\n  0% {\n    background-size: 650%;\n  }\n\n  40% {\n    background-size: 650%;\n  }\n\n  100% {\n    background-size: 100%;\n  }\n}\n\n@keyframes simple-text-gradient-rev {\n  0% {\n      background-size: 100%;\n  }\n  80% {\n      background-size: 650%;\n  }\n  100% {\n      background-size: 650%;\n  }\n}\n\n@media (max-width: 599px) {\n\n  body {\n    min-height: 180vh;\n  }\n\n  header {\n    max-width: 350px;\n  }\n\n  header h1 {\n    font-size: 4rem;\n    margin: 2rem auto;\n  }\n\n  section.pomodoro {\n    width: 95%;\n  }\n\n  .settings {\n    width: 95%;\n    grid-template-columns: 1fr;\n    grid-template-rows: 1fr 2fr;\n  }\n\n  .settings .work-duration {\n    grid-row: 2 / span 1;\n  }\n\n  .reset-btn {\n    grid-row: 1;\n    margin-bottom: 2rem;\n  }\n\n  .to-do {\n    width: 95%;\n    grid-template-columns: 1fr 100fr 1fr;\n    grid-gap: .8rem;\n    min-height: 120px;\n  }\n\n  .to-do form {\n    grid-template-columns: 3fr 1fr;\n  }\n\n  .to-do form input {\n    font-size: 1.2rem;\n    min-height: 3rem;\n  }\n\n  .to-do form button {\n    font-size: 1.5rem;\n    min-height: 3rem;\n  }\n}\n\n\n\n\n"},